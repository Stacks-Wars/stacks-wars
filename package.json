{
  "name": "stacks-wars",
  "private": true,
  "type": "module",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "dotenv": "^17.2.2",
      "zod": "^4.1.13",
      "@tanstack/react-query": "^5.90.12",
      "better-auth": "1.4.5",
      "@better-auth/expo": "1.4.5"
    }
  },
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "check-types": "turbo check-types",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "db:push": "turbo -F @stacks-wars/db db:push",
    "db:studio": "turbo -F @stacks-wars/db db:studio",
    "db:generate": "turbo -F @stacks-wars/db db:generate",
    "db:migrate": "turbo -F @stacks-wars/db db:migrate"
